{
  "name": "wc-card",
  "version": "0.0.0",
  "description": "Webcomponent wc-card following open-wc recommendations",
  "author": "wc-card",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch",
    "prepublish": "tsc",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "wc-theme": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@open-wc/demoing-storybook": "^2.4.7",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/web-components": "^6.1.15",
    "@web/dev-server": "^0.1.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "lit-html": "^1.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
