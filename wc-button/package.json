{
  "name": "wc-button",
  "version": "0.0.1",
  "description": "Webcomponent wc-button following open-wc recommendations",
  "author": "wc-button",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch",
    "prepublish": "tsc",
    "storybook": "concurrently --kill-others --names tsc,storybook \"npm run tsc:watch\" \"start-storybook --node-resolve --watch --open\"",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "wc-theme": "0.0.1",
    "wc-icon": "0.0.1"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "@web/dev-server": "^0.0.12",
    "typescript": "~4.0.3",
    "concurrently": "^5.1.0",
    "tslib": "^1.11.0",
    "@open-wc/demoing-storybook": "^2.0.0"
  }
}
